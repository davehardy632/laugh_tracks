
<% if @comedian.errors.any? %>
<% @comedian.errors.full_messages.each do |message| %>
<li><%= message %></li>
<% end %>
<% end %>

<%= form_for @comedian do |f| %>
<ul>

  <li><%= f.label :name %></li><br/>
  <li><%= f.text_field :name %></li>
</ul>
<ul>
  <li><%= f.label :age %></li><br/>
  <li><%= f.number_field :age %></li>
</ul>
<ul>
  <li><%= f.label :birthplace %></li><br/>
  <li><%= f.text_field :birthplace %></li>
</ul>
<ul>
  <li><%= f.label :image_url %></li><br/>
  <li><%= f.text_field :image_url %></li>
</ul>
<ul>
  <li><%= f.submit :Submit %></li>
</ul>
<% end %>
